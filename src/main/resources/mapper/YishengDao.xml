<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.YishengDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.YishengEntity" id="yishengMap">
        <result property="gonghao" column="gonghao"/>
        <result property="mima" column="mima"/>
        <result property="yishengxingming" column="yishengxingming"/>
        <result property="xingbie" column="xingbie"/>
        <result property="shenfenzheng" column="shenfenzheng"/>
        <result property="shoujihaoma" column="shoujihaoma"/>
        <result property="xiangpian" column="xiangpian"/>
        <result property="yiyuanmingcheng" column="yiyuanmingcheng"/>
        <result property="yiyuandizhi" column="yiyuandizhi"/>
        <result property="keshimingcheng" column="keshimingcheng"/>
        <result property="keshidizhi" column="keshidizhi"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.YishengVO" >
		SELECT * FROM yisheng  yisheng         
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.YishengVO" >
		SELECT  yisheng.* FROM yisheng  yisheng 	
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.YishengView" >

		SELECT  yisheng.* FROM yisheng  yisheng 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.YishengView" >
		SELECT * FROM yisheng  yisheng <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

</mapper>
